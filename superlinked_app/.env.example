# Copy this file to .env and fill in your actual values

# OpenAI Configuration for MistralAI (for Natural Language Queries) (Also works with other providers like OpenAI)
OPENAI_API_KEY=your_mistral_ai_api_key_here
OPENAI_MODEL=mistral-tiny
OPENAI_BASE_URL=https://api.mistral.ai/v1/

# Qdrant Vector Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Text Embedder Model
TEXT_EMBEDDER_NAME=ibm-granite/granite-embedding-small-english-r2

# Data Processing
CHUNK_SIZE=512
